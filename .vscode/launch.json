{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "main.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/maps4fsbot/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
